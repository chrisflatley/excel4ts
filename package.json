{
  "name": "excel4node",
  "version": "1.8.0",
  "description": "Library to create Formatted Excel Files.",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "excel",
    "spreadsheet",
    "xlsx",
    "formatted",
    "styled",
    "report",
    "workbook",
    "ooxml"
  ],
  "main": "./distribution/index.js",
  "author": {
    "name": "Nater",
    "email": "nater@iamnater.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:advisr-io/excel4node.git"
  },
  "bugs": {
    "url": "https://github.com/advisr-io/excel4node/labels/bug"
  },
  "scripts": {
    "test": "NODE_ENV=test tape --ts ./tests/*.test.js",
    "cover": "nyc tape --ts ./tests/*.test.js",
    "build": "tsc",
    "watch": "tsc -w",
    "document": "jsdoc ./source -r -d docs",
    "prepublishOnly": "npm run build; npm run test",
    "validate": "npm test; npm run build; node sample.js; ./validate.sh Excel.xlsx"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "image-size": "^1.0.2",
    "jszip": "^3.10.1",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isundefined": "^3.0.1",
    "lodash.reduce": "^4.6.0",
    "lodash.uniqueid": "^4.0.1",
    "mime": "^3.0.0",
    "uuid": "^9.0.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/node": "18.11.7",
    "@xmldom/xmldom": "^0.8.3",
    "jsdoc": "^3.6.11",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "tape": "^5.6.1",
    "tape-promise": "^4.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "xpath.js": "^1.1.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "sourceMap": false,
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
